package org.hatice.ikplus.repository.employeerepository.;

import org.hatice.ikplus.entity.companymanagement.Company;
import org.springframework.data.jpa.repository.JpaRepository;

import java.util.List;

public interface EmployeeRepository extends JpaRepository<Company, Long> {
	
	List<Company> findByIsApproved(boolean isApproved);
}